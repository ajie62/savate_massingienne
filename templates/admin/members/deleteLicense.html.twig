{% extends 'admin/base.html.twig' %}

{% block admin_body %}

    <h1>admin.delete_license</h1>

    <p>
        You're about to delete the
        {{ license.year }}/{{ license.year + 1 }}
        license that belongs to <strong>{{ license.user == app.user ? 'you' : license.user.username|capitalize }}</strong>.
        Do you confirm?
    </p>

    {{ form_start(form) }}
    {{ form_widget(form) }}
    <button type="submit">Confirm</button>
    {{ form_end(form) }}

{% endblock %}

